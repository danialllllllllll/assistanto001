run = "python train_advanced_ai.py"
modules = ["python-3.12", "web", "bash"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "sequential"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "AI Training System"

[[workflows.workflow]]
name = "AI Training System"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python train_advanced_ai.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[nix]
channel = "stable-25_05"
packages = ["cairo", "ffmpeg-full", "freetype", "ghostscript", "glibcLocales", "gobject-introspection", "gtk3", "openssl", "pkg-config", "postgresql", "qhull", "run", "tcl", "tk"]

[deployment]
run = ["sh", "-c", "python train_advanced_ai.py"]
deploymentTarget = "autoscale"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 3000

[agent]
expertMode = true
